
    <!-- breadcrumb start-->
    <section class="breadcrumb breadcrumb_bg">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12">
                    <div class="breadcrumb_iner">
                        <div class="breadcrumb_iner_item">
                            <p>Trang chủ/ Điện tử / Điện thoại di động</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb start-->
    <!--================Single Product Area =================-->
    <div class="product_image_area section_padding">
        <div class="container">
            <div class="row s_product_inner">
                <div class="col-lg-5">
                    <div class="product_slider_img">
                        <div id="vertical">
                            {{#each this.ImgPath}}
                            <div data-thumb={{this}}>
                                <img src={{this}} />
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <div class="col-lg-5 offset-lg-1">
                    <div class="s_product_text">
                        <h2>{{ProductName}}</h2>
                        <h3>Giá hiện tại: {{Price}} VND</h3>
                        <h2>Giá mua ngay: {{BuyNow}} VND</h2>
                        <ul class="list">
                            <li>
                                <a class="active" href="#">
                                    <span>Thể loại: {{Type}}</a>
                            </li>
                            <li>
                                <a href="#">
                                    <span>Người ra giá: {{Bidder}}</span>
                                    <span> {{BidderScore}}/10</span></a>
                            </li>
                            <li>
                                <a href="#"> <span>Người bán: {{Seller}} {{SellerScore}}/10</span></a>
                            </li>
                            <li>
                                <div>Thời gian đăng: {{Time}}</div>
                            </li>
                            <li>
                                <div>Thời gian kết thúc: {{TimeUp}}</div>
                            </li>
                        </ul>
                        <p>
                            {{ProductDescription}}<div class="card_area">
                            <div class="product_count d-inline-block">
                                <input class="input-number priceformat" id="price_auction" type="number" maxlength="5">
                                <span>VND</span>
                            </div>
                            <div class="add_to_cart">
                                <a href="#" class="btn_3" id="Auction">Đấu giá</a>
                                <div class="text-danger" id="Invalid-price">Giá không hợp lệ </div>
                                <a href="cart.html" class="btn_3 ml-3" id="Auction">Mua ngay</a>
                                <a href="#" class="like_us"> <i class="ti-heart"></i> </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--================End Single Product Area =================-->
    <!-- history bidder part -->
    <section class="col-lg-12">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12">
                    <div class="section_tittle text-center">
                        <h2>Lịch sử đấu giá</h2>
                    </div>
                </div>
            </div>
            <table class="table table-striped table-dark table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th scope="col">STT</th>
                        <th scope="col">Người đấu giá</th>
                        <th scope="col">Giá</th>
                    </tr>
                </thead>
                <tbody id="history-table">
                    {{#each this.historyBidder}}
                    <tr>
                        <th scope="row">{{id}}</th>
                        <td>{{Name}}</td>
                        <td>{{BidValue}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </section>
    <!-- history bidder end       -->

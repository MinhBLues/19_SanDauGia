 <section class="banner_part">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <div class="banner_slider">
                        <div class="single_banner_slider">
                            <div class="banner_text">
                                <div class="banner_text_iner">
                                    <h1>Sàn đấu giá trực tuyến</h1>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
<section class="cat_product_area section_padding border_top">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="product_top_bar d-flex justify-content-between align-items-center">
                            <div class="single_product_menu product_bar_item">
                                <h2>Top 5 sản phẩm gần kết thúc</h2>
                            </div>
                        </div>
                    </div>
                    {{#each this.TimeUp}}
                    <div class="col-lg-4 col-sm-6">
                        <div class="single_category_product">
                            <div class="single_category_img">
                                <img height="340" width="340" src="{{ImgPath}}" alt="">
                                <div class="category_social_icon">
                                    <ul>
                                        <li><a href="#"><i class="ti-heart"></i></a></li>
                                        <li><a href="#"><i class="ti-bag"></i></a></li>
                                    </ul>
                                </div>
                                <div class="category_product_text">
                                    <a href="detail/{{id}}">
                                        <h5>{{Name}}</h5>
                                    </a>
                                    <p>{{Price}} VND</p>
                                    <p>{{TimeUp}}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="product_top_bar d-flex justify-content-between align-items-center">
                                    <div class="single_product_menu product_bar_item">
                                        <h2>Top 5 sản phẩm có nhiều lượt ra giá nhất</h2>
                                    </div>
                                </div>
                            </div>
                            {{#each this.popular}}
                            <div class="col-lg-4 col-sm-6">
                                <div class="single_category_product">
                                    <div class="single_category_img">
                                        <img height="340" width="340" src="{{ImgPath}}" alt="">
                                        <div class="category_social_icon">
                                            <ul>
                                                <li><a href="#"><i class="ti-heart"></i></a></li>
                                                <li><a href="#"><i class="ti-bag"></i></a></li>
                                            </ul>
                                        </div>
                                        <div class="category_product_text">
                                            <a href="detail.html">
                                                <h5>{{Name}}</h5>
                                            </a>
                                            <p>{{Price}} VND</p>
                                            <p>{{TimeUp}}</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="product_top_bar d-flex justify-content-between align-items-center">
                                    <div class="single_product_menu product_bar_item">
                                        <h2>Top 5 sản phẩm có giá cao nhất</h2>
                                    </div>
                                </div>
                            </div>
                            {{#each this.Expensive}}
                            <div class="col-lg-4 col-sm-6">
                                <div class="single_category_product">
                                    <div class="single_category_img">
                                        <img height="340" width="340" src="{{ImgPath}}" alt="">
                                        <div class="category_social_icon">
                                            <ul>
                                                <li><a href="#"><i class="ti-heart"></i></a></li>
                                                <li><a href="#"><i class="ti-bag"></i></a></li>
                                            </ul>
                                        </div>
                                        <div class="category_product_text">
                                            <a href="detail.html">
                                                <h5>{{Name}}</h5>
                                            </a>
                                            <p>{{Price}} VND</p>
                                            <p>{{TimeUp}}</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
</section>